{
  "name": "minion-renderer",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "docker-compose:restart": "run-s docker-compose:build docker-compose:stop docker-compose:start",
    "docker-compose:build": "docker-compose -f Docker/docker-compose.yaml build --no-cache",
    "docker-compose:start": "docker-compose -f Docker/docker-compose.yaml up",
    "docker-compose:stop": "docker-compose -f Docker/docker-compose.yaml down",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix -f visualstudio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "git": "run-s git:add git:commit git:push",
    "git:commit": "git commit -m \"update dependencies\"",
    "git:add": "git add package.json package-lock.json",
    "git:push": "git push",
    "dependencies:update": "ncu -u -x typescript,ngx-mqtt",
    "dependencies:install": "npm install",
    "update:dependencies and build": "run-s dependencies:update dependencies:install build git"
  },
  "dependencies": {
    "@nestjs/common": "7.6.14",
    "@nestjs/core": "7.6.14",
    "@nestjs/platform-express": "7.6.14",
    "@nestjs/serve-static": "2.1.4",
    "@nestjs/swagger": "4.7.16",
    "chance": "1.1.7",
    "chroma-js": "2.1.1",
    "reflect-metadata": "0.1.13",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "request-ip": "2.1.3",
    "rxjs": "6.6.6",
    "swagger-ui-express": "4.1.6",
    "uuid": "8.3.2",
    "xml2js": "0.4.23",
    "xmldom": "0.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.6",
    "@nestjs/microservices": "7.6.17",
    "@nestjs/schematics": "7.3.0",
    "@nestjs/testing": "7.6.14",
    "@types/chance": "1.1.1",
    "@types/chroma-js": "2.1.3",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.35",
    "@types/request-ip": "0.0.35",
    "@types/supertest": "2.0.10",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "adblockbanner": "1.1.0",
    "babel-loader": "8.2.2",
    "cache-manager": "3.4.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "extract-text-webpack-plugin": "3.0.2",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "sharp": "0.27.2",
    "supertest": "6.1.3",
    "npm-check-updates": "11.3.0",
    "ts-jest": "26.5.3",
    "ts-loader": "8.0.18",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.3",
    "webpack-node-externals": "2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
